/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;
    
    PARTS:
    // Put your code here:
    Nand (a=a, b=sel, out=t0);
    Not (in=sel, out=t1);
    Nand (a=a, b=t1, out=t2);
    Nand (a=t0, b=t2, out=out);
}