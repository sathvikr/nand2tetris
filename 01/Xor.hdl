/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand (a=a, b=b, out=t0);
    Or (a=a, b=b, out=t1);
    And (a=t0, b=t1, out=out);
}